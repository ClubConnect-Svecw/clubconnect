        <activity android:name="com.ruuha.bmscms.StudentDetailsInput" >
            <intent-filter>
                <action android:name="android.intent.action.M" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.ruuha.bmscms.StudentList"
